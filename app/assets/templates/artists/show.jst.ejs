<div class="row">
  <div class="col-xs-4">
    <div id="name_and_message">
      <h1><%= artist.get("artistname") %></h1>
    </div>
      
    <div id="avatar">
      <img class="avatar_large" src="<%= artist.get("image_url") %>"></img>
    </div>
    
    <div id="about_me" class="sidebar_unit">
      <div id="label">
        <% if (artist.get("artistname")) { %>
          ABOUT <%= artist.get("artistname").toUpperCase() %>
        <% } %>
        
      </div>
      <%= artist.get("about") %>
    </div>
  </div>
  
  <div class="col-xs-7 col-xs-offset-1">
    <ul>
      <% for (var i = 0; i < tracks.length; i++) { %>
        <li>
          <a href="<%= '#/tracks/' + tracks[i].id %>">
          <%= tracks[i].get('title') %>
        </li>
      <% } %>
    </ul>
  </div>
</div>