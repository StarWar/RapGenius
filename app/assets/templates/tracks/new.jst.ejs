<h1>ADD A NEW TRACK</h1>

<form role="form" class="form-horizontal" autocomplete="off">
  <div class="form-group col-xs-7">
    <label>Primary Artist
      <input type="text" name="artistname" id="artist_name"                           data-provide="typeahead" class="typeahead form-control">
    </label>
    <label>Title
      <input type="text" name="track[title]" class="form-control">
    </label>
  </div>
  <div class="form-group col-xs-12">
    <label>About
      <textarea class="form-control" name="track[about]" rows="20"></textarea>
    </label>
    <label>Lyrics
      <textarea class="form-control" name="track[lyrics]" rows="60"></textarea>
    </label>
    <button type="submit" class="btn btn-default">Submit</button>
  </div>
</form>